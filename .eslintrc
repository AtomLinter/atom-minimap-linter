{
  "env": {
    "browser": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
    "atom": false
  },
  "rules": {
    "camelcase": 1,
    "comma-spacing": 1,
    "curly": [1, "multi-line"],
    "dot-notation": 1,
    "strict": 1,
    "key-spacing": 1,
    "no-console": 0,
    "no-extra-bind": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": 1,
    "no-spaced-func": 1,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 1,
    "no-unused-vars": 1,
    "quotes": 0,
    "semi-spacing": 1,
    "space-infix-ops": 1,
    "space-unary-ops": 1,
  }
}
